[sub_resource type="GDScript" id=1]

script/source = "extends Spatial\n\nonready var player=get_node(\"player\")\n\nfunc _ready():\n\t\n\tplayer.get_data().set_modifier(\"attack.elemental_chance\",1)\n\n\tset_process_input(true)\n\nfunc _input(event):\n\tif Input.is_action_pressed(\"ui_cancel\"):\n\t\tget_tree().quit()\n\nfunc _on_firearea_body_enter( body ):\n\tif body==player:\n\t\tplayer.get_data().set_modifier(\"attack.elemental_impact\",\"fire\")\n\t\tplayer.get_data().reset_pool()\n\nfunc _on_acidarea_body_enter( body ):\n\tif body==player:\n\t\tplayer.get_data().set_modifier(\"attack.elemental_impact\",\"acide\")\n\t\tplayer.get_data().reset_pool()\n\n\nfunc _on_explosionarea_body_enter( body ):\n\tif body==player:\n\t\tplayer.get_data().set_modifier(\"attack.elemental_impact\",\"explosion\")\n\t\tplayer.get_data().reset_pool()\n"

