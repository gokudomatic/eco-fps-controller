script/source = "extends Spatial\n\nvar selected_weapon=null\nvar selected_weapon_profile=null\n\nvar weapons={\n\t0:[\"beam\",\"ball\"],\n\t1:[\"pistol\",\"magic wand\"],\n\t2:[\"flame\"],\n\t3:[\"rocket\",\"grenade\"],\n\t4:[\"laser\",\"bolt\"]\n}\n\nvar weapon_profiles={\n\t\"beam\":{ \"ammo\":100,\"max_ammo\":100,\"ammo_loaded\":8,\"max_ammo_loaded\":8,\"fire_rate\":3,\"reload_time\":2},\n\t\"ball\":{ \"ammo\":-1,\"max_ammo\":-1,\"ammo_loaded\":-1,\"max_ammo_loaded\":-1,\"fire_rate\":3,\"reload_time\":2},\n\t\"pistol\":{ \"ammo\":100,\"max_ammo\":100,\"ammo_loaded\":10,\"max_ammo_loaded\":10,\"fire_rate\":3,\"reload_time\":2},\n\t\"magic wand\":{ \"ammo\":-1,\"max_ammo\":-1,\"ammo_loaded\":-1,\"max_ammo_loaded\":-1,\"fire_rate\":3,\"reload_time\":2},\n\t\"flame\":{ \"ammo\":500,\"max_ammo\":500,\"ammo_loaded\":100,\"max_ammo_loaded\":100,\"fire_rate\":0.3,\"reload_time\":2},\n\t\"rocket\":{ \"ammo\":40,\"max_ammo\":40,\"ammo_loaded\":3,\"max_ammo_loaded\":3,\"fire_rate\":3,\"reload_time\":2},\n\t\"grenade\":{ \"ammo\":50,\"max_ammo\":50,\"ammo_loaded\":5,\"max_ammo_loaded\":5,\"fire_rate\":3,\"reload_time\":2},\n\t\"laser\":{ \"ammo\":-1,\"max_ammo\":-1,\"ammo_loaded\":-1,\"max_ammo_loaded\":-1,\"fire_rate\":3,\"reload_time\":2},\n\t\"bolt\":{ \"ammo\":-1,\"max_ammo\":-1,\"ammo_loaded\":50,\"max_ammo_loaded\":50,\"fire_rate\":0.3,\"reload_time\":2}\n}\n\nfunc _ready():\n\t\n\tvar data=get_node(\"player\").get_data()\n\tdata.bullet_pool_capacity=6\n\tdata.reset_pool()\n\t\n\tset_process_input(true)\n\nfunc _input(event):\n\tif Input.is_action_pressed(\"ui_cancel\"):\n\t\tget_tree().quit()\n\t\n\tif Input.is_key_pressed(KEY_1):\n\t\tselect_weapon(0)\n\telif Input.is_key_pressed(KEY_2):\n\t\tselect_weapon(1)\n\telif Input.is_key_pressed(KEY_3):\n\t\tselect_weapon(2)\n\telif Input.is_key_pressed(KEY_4):\n\t\tselect_weapon(3)\n\telif Input.is_key_pressed(KEY_5):\n\t\tselect_weapon(4)\n\nfunc select_weapon(num):\n\tsave_profile()\n\t\n\tvar rack=weapons[num]\n\tvar i=rack.find(selected_weapon)\n\tvar new_i=0\n\tif i>-1:\n\t\tnew_i=i+1\n\t\tif new_i>=rack.size():\n\t\t\tnew_i=0\n\t\t\n\tselected_weapon=rack[new_i]\n\tget_node(\"player\").weapon=selected_weapon\n\tselected_weapon_profile=weapon_profiles[selected_weapon]\n\tload_profile()\n\nfunc load_profile():\n\tvar data=get_node(\"player\").get_data()\n\tdata.fire_rate=selected_weapon_profile.fire_rate\n\tdata.reload_time=selected_weapon_profile.reload_time\n\tdata.bullet_pool_capacity=selected_weapon_profile.max_ammo_loaded\n\tdata.bullet_max_capacity=selected_weapon_profile.max_ammo\n\tget_node(\"player\").set_ammo(selected_weapon_profile.ammo)\n\tget_node(\"player\").set_loaded_ammo(selected_weapon_profile.ammo_loaded)\n\tdata.reset_pool()\n\t\n\nfunc save_profile():\n\tif selected_weapon_profile==null:\n\t\treturn\n\t\t\n\tvar data=get_node(\"player\").get_data()\n\t\n\tselected_weapon_profile.ammo=get_node(\"player\").get_ammo()\n\tselected_weapon_profile.ammo_loaded=get_node(\"player\").get_loaded_ammo()\n\t\n\nfunc _on_player_reload_weapon():\n\tpass # replace with function body\n"

