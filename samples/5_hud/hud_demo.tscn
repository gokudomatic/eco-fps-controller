script/source = "extends CanvasLayer\n\nvar weapon_label_mapping={\n\t\"beam\":\"weapon_0_0\",\n\t\"ball\":\"weapon_0_1\",\n\t\"pistol\":\"weapon_1_0\",\n\t\"magic wand\":\"weapon_1_1\",\n\t\"flame\":\"weapon_2_0\",\n\t\"rocket\":\"weapon_3_0\",\n\t\"grenade\":\"weapon_3_1\",\n\t\"laser\":\"weapon_4_0\",\n\t\"bolt\":\"weapon_4_1\"\n}\n\nvar selected_weapon_label=null\n\nfunc _ready():\n\tpass\n\n\nfunc _on_player_attribute_changed( key, value ):\n\tvar n=null\n\tif key==\"life\":\n\t\tn=get_node(\"lbl_life\")\n\telif key==\"shield\":\n\t\tn=get_node(\"lbl_shield\")\n\telif key==\"nb_bullets\":\n\t\tn=get_node(\"lbl_ammo\")\n\t\tvalue=str(value)+\" / \"+str(.get_node(\"/root/world/player\").get_ammo())\n\telif key==\"weapon\" and value!=selected_weapon_label:\n\t\tvar n1=get_node(\"hud_weapons/\"+weapon_label_mapping[value])\n\t\tif n1!=null: # if it\'s valid, set it\n\t\t\tif selected_weapon_label!=null:\n\t\t\t\tget_node(\"hud_weapons/\"+weapon_label_mapping[selected_weapon_label]).add_color_override(\"font_color\",Color(1,1,1))\n\t\t\tselected_weapon_label=value\n\t\t\tn1.add_color_override(\"font_color\",Color(1,1,0))\n\t\t\tget_node(\"AnimationPlayer\").play(\"hud_display\")\n\t\n\tif n!=null:\n\t\tn.set_text(str(value))\n"

